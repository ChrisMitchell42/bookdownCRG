	Issn = {2211-1247},
	Journal = {Cell Reports},
	Keywords = {canine, domestication, population, migration, morphology, behavior, admixture},
	Number = {4},
	Pages = {697 - 708},
	Title = {Genomic Analyses Reveal the Influence of Geographic Origin, Migration, and Hybridization on Modern Dog Breed Development},
